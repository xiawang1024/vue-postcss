{
	"name": "live-player",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"better-scroll": "^1.13.0",
		"fastclick": "^1.0.6",
		"hls.js": "^0.11.0",
		"store": "^2.0.12",
		"vue": "^2.5.17",
		"vue2-better-scroll": "^1.0.7",
		"vue2-simplert": "^0.8.0",
		"weixin-js-sdk": "^1.3.3"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.0",
		"@vue/cli-service": "^3.0.0",
		"cssnano": "^4.1.3",
		"cssnano-preset-advanced": "^4.0.1",
		"node-sass": "^4.9.0",
		"postcss-aspect-ratio-mini": "^0.0.2",
		"postcss-import": "^12.0.0",
		"postcss-preset-env": "^6.0.6",
		"postcss-px-to-viewport": "^0.0.3",
		"postcss-url": "^8.0.0",
		"postcss-viewport-units": "^0.1.4",
		"postcss-write-svg": "^3.0.1",
		"sass-loader": "^7.0.1",
		"vue-template-compiler": "^2.5.17"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {},
			"postcss-import": {},
			"postcss-url": {},
			"postcss-preset-env": {},
			"postcss-aspect-ratio-mini": {},
			"postcss-write-svg": {
				"utf8": false
			},
			"postcss-px-to-viewport": {
				"viewportWidth": 750,
				"viewportHeight": 1334,
				"unitPrecision": 3,
				"viewportUnit": "vw",
				"selectorBlackList": [
					".ignore",
					".hairlines"
				],
				"minPixelValue": 1,
				"mediaQuery": false
			},
			"postcss-viewport-units": {},
			"cssnano": {
				"preset": "advanced",
				"autoprefixer": false,
				"postcss-zindex": false
			}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
